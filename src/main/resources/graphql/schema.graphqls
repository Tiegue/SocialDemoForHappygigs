type Message{
    sender: String!
    receiver: String!
    content: String!
    timestamp: String!
}
type Query{
    test:String
}

type Mutation {
    userEnteredVenue(userId: String!, venueId: String!): String
    userLeftVenue(userId: String!, venueId: String!): String

    sendMessage(sender: String!, receiver: String!, content: String!): Message
}


type Subscription{
    receiveMessages(userId: String!): Message
    receiveUserList(userId: String!): [String]
}